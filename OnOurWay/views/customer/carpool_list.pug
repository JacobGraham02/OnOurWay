extends ../layout

block content

  article#list_of_user_carpools_container 
    ul#list_of_user_carpools 
      -for (let i = 0; i < user_carpool_information.length; i++) {
        li#user_carpool_list_item
          a(href=`/carpool/detailed_route?id=${user_carpool_information[i].id}`)= "Carpool starting address: " + user_carpool_information[i].starting_address + " Carpool ending address: " + user_carpool_information[i].ending_address
      -}